   </license>

   <url>
      <update label="Jif Updates"
            url="https://www.cs.cornell.edu/jif/ide/updates/"/>
      <discovery label="Jif project page"
            url="https://www.cs.cornell.edu/jif/"/>
   </url>

   <requires>
      <import plugin="jif"/>
      <import plugin="polyglot.ide"/>
   </requires>

   <plugin
         id="jif.ide"
         download-size="0"
         install-size="0"
         version="@version@"
         unpack="false"/>
